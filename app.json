{
  "expo": {
    "name": "Trifle",
    "slug": "trifle",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "mdpi": "./assets/splash/mdpi/splash.png",
      "hdpi": "./assets/splash/hdpi/splash.png",
      "xhdpi": "./assets/splash/xhdpi/splash.png",
      "xxhdpi": "./assets/splash/xxhdpi/splash.png",
      "xxxhdpi": "./assets/splash/xxxhdpi/splash.png",
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.trifle.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 11
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": ""
  }
}
